<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartapp">

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="com.google.android.safetynet.ATTEST_API_KEY"
        android:value="@string/api_key" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".Home"></activity>
        <activity android:name=".LED_activity" />
        <activity android:name=".VerifySecureCode" />
        <activity android:name=".SecurityCode" />
        <activity android:name=".FingerPrint" />
        <activity android:name=".VerifyPhone" />
        <activity android:name=".Register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>